{
	"author": "Elisandro M Correa",
	"name": "skeleton-sub",
	"description": "The SkeletonSub Microservices Framework is designed to provide a base framework for implementing Subscriber microservices on AWS SQS queues.",
	"version": "1.0.0",
	"main": "index.js",
	"keywords": [
		"SkeletonSub",
		"BreathingCode",
		"Clean Architecture",
		"TypeScript",
		"DDD",
		"iamElisandroCorrea"
	],
	"license": "MIT",
	"type": "commonjs",
	"dependencies": {
		"axios": "^1.8.4",
		"@aws-sdk/client-sqs": "^3.699.0",
		"env-var": "^7.5.0",
		"module-alias": "^2.2.3",
		"rimraf": "^6.0.1",
		"validator": "^13.15.0"
	},
	"scripts": {
		"check": "npm-check -s -u",
		"dev": "node -r ts-node/register --env-file=.env src/main/index.ts",
		"clean": "rimraf dist",
		"build": "npm run clean && tsc -p tsconfig-build.json",
		"start": "node -r dotenv/config dist/main/index",
		"lint": "npx @biomejs/biome check src",
		"lint:format": "npx @biomejs/biome check --write src",
		"test": "jest --passWithNoTests --no-cache --runInBand"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@jest/globals": "^29.7.0",
		"@types/aws-lambda": "^8.10.149",
		"@types/jest": "^29.5.14",
		"@types/module-alias": "^2.0.4",
		"@types/node": "^22.14.1",
		"@types/validator": "^13.15.0",
		"glob": "^11.0.1",
		"jest": "^29.7.0",
		"ts-jest": "^29.3.2",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.3"
	},
	"overrides": {
		"glob": "^11.0.1"
	}
}
